@model IEnumerable<Firma.Data.Data.CMS.ZdjecieGaleria>


<div class="col-12 text-center">
    <h2 class="mb-5 fw-bold dark-rusty-color">Galeria naszych wypieków</h2>
    <div class="row g-4">
        @foreach (var item in Model)
        {
            <div class="col-md-4 gallery-item">
                <img src=@item.AdresURL alt=@item.Nazwa class="img-fluid product-img rounded shadow">
            </div>
        }
    </div>
</div>
